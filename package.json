{
  "name": "brackets-eslint",
  "version": "2.9.5",
  "title": "ESLint",
  "description": "Adds ESLint support to Brackets",
  "keywords": [
    "brackets-extension",
    "eslint",
    "lint",
    "linting",
    "linter"
  ],
  "homepage": "https://github.com/zaggino/brackets-eslint",
  "bugs": "https://github.com/zaggino/brackets-eslint/issues",
  "license": "MIT",
  "author": {
    "name": "Martin Zagora",
    "email": "zaggino@gmail.com",
    "url": "https://github.com/zaggino"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zaggino/brackets-eslint.git"
  },
  "scripts": {
    "dev": "tsc --watch",
    "test": "typings install && tsc && tslint -c tslint.json --project tsconfig.json",
    "prepublish": "typings install && rimraf dist && tsc"
  },
  "dependencies": {
    "brackets-inspection-gutters": "^0.2.0",
    "enable-global-packages": "latest",
    "eslint": "<3.0.0"
  },
  "devDependencies": {
    "tslint": "^3.14.0",
    "typescript": "^2.0.0",
    "typings": "^1.3.2"
  },
  "engines": {
    "brackets": ">=1.2.0"
  }
}
